{
    "name": "facebook/hack-router-codegen",
    "description": "URI routing for Hack with codegen",
    "keywords": ["hack", "router", "routing", "hhvm"],
    "homepage": "https://github.com/hhvm/hack-router-codegen",
    "require": {
        "facebook/hack-router": "^1.0.0rc7",
        "facebook/hack-codegen": "dev-master",
        "facebook/definition-finder": "^1.3.0",
        "fredemmott/type-assert": "^0.2.0 || ^1.0"
    },
    "require-dev": {
        "91carriage/phpunit-hhi": "^5.5",
        "phpunit/phpunit": "^5.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
      "classmap": [ "src/" ]
    },
    "autoload-dev": {
      "classmap": [ "tests/" ]
    }
}
