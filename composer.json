{
    "name": "fredemmott/hack-router-codegen",
    "description": "URI routing for Hack with codegen",
    "keywords": ["hack", "router", "routing", "hhvm"],
    "homepage": "https://github.com/fredemmott/hack-router-codegen",
    "require": {
        "fredemmott/hack-router": "dev-master",
        "facebook/hack-codegen": "^1.1",
        "fredemmott/definition-finder": "dev-master",
        "hhvm/systemlib-extractor": "^0.1.0"
    },
    "require-dev": {
        "91carriage/phpunit-hhi": "^5.5",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
      "classmap": [ "src/" ]
    },
    "autoload-dev": {
      "classmap": [ "tests/" ]
    }
}
