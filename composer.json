{
    "name": "facebook/hack-router-codegen",
    "description": "URI routing for Hack with codegen",
    "keywords": ["hack", "router", "routing", "hhvm"],
    "homepage": "https://github.com/hhvm/hack-router-codegen",
    "require": {
        "hhvm": "^3.28.0",
        "facebook/hack-router": "dev-master",
        "facebook/hack-http-request-response-interfaces": "dev-master",
        "facebook/hack-codegen": "^4.0",
        "facebook/definition-finder": "^2.0",
        "hhvm/type-assert": "^3.0"
    },
    "require-dev": {
        "hhvm/hhast": "^3.28",
        "hhvm/hacktest": "^1.1",
        "facebook/fbexpect": "^2.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
      "classmap": [ "src/" ]
    },
    "autoload-dev": {
      "classmap": [ "tests/" ]
    }
}
